<p id="notice"><%= notice %></p>

<h1>Listing Repositories</h1>

<table>
  <thead>
    <tr>
      <th>Iv</th>
      <th>Title</th>
      <th>Master key</th>
      <th>Pass</th>
      <th>Token</th>
      <th>Description</th>
      <th>Created</th>
      <th>Deletion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @repositories.each do |repository| %>
      <tr>
        <td><%= repository.iv %></td>
        <td><%= repository.title %></td>
        <td><%= repository.master_key %></td>
        <td><%= repository.pass %></td>
        <td><%= repository.token %></td>
        <td><%= repository.description %></td>
        <td><%= repository.created %></td>
        <td><%= repository.deletion %></td>
        <td><%= link_to 'Show', repository %></td>
        <td><%= link_to 'Edit', edit_repository_path(repository) %></td>
        <td><%= link_to 'Destroy', repository, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Repository', new_repository_path %>
